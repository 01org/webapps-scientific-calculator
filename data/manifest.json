{
  "name": "Calculator",
  "version": "2.0",
  "default_locale": "en_US",
  "description": "A scientific calculator",
  "icons": {
    "128": "icon_128.png"
  },
  "app": {
    "launch": {
        "local_path": "index.html"
    }
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; style-src 'self' 'unsafe-inline'"
}

